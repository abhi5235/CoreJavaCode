package com.collection;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class Collect {

	public static void main(String[] args) {
Employee e1=new Employee(111, "Abhishek",10000);
Employee e2=new Employee(112, "Deepak",20000);
Employee e3=new Employee(113, "Aarav",30000);
Employee e4=new Employee(114, "Rohit",40000);

List<Employee> list=new ArrayList<>();
list.add(e1);
list.add(e2);
list.add(e3);
list.add(e4);

Collections.max(list.(x,y)->(x.id-y.id));
System.out.println(list);
	}

}
